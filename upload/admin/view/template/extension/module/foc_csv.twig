{{ header }}
{{ column_left }}

<div id="content">

  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <a href="" data-toggle="tooltip" title="" class="btn btn-default" data-original-title="Отменить"><i class="fa fa-reply"></i></a></div>

      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
      {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
      {% endfor %}
      </ul>
    </div>
  </div>

  <div class="container-fluid">
    <div id="foc_csv">
      <div class="container">
        <div class="panel panel-default">
          <div class="panel-content">
            <h3>{{ foc_app_preload_title }}</h3>
            <p>{{ foc_app_preload_description }}</p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
// Настройки для приложения
window.FOC_CSV_PARAMS = {
  'requestConfig' : {
    'token': '{{ token }}',
    'tokenName': '{{ tokenName }}',
    'baseRoute': '{{ baseRoute }}',
    'baseUrl': '{{ baseUrl }}',
  },
  'language': '{{ language }}',
  'initial' : {{ initial }}
};
</script>
{% for script in scripts %}
  <script src="{{ script }}" type="text/javascript"></script>
{% endfor %}

{{ footer }}