{{ header }}
{{ column_left }}

<div id="content">

  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <a href="" data-toggle="tooltip" title="" class="btn btn-default" data-original-title="Отменить"><i class="fa fa-reply"></i></a></div>

      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
      {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.name }}</a></li>
      {% endfor %}
      </ul>
    </div>
  </div>

  <div class="container-fluid">
    <div id="foc_csv">FOC_CSV IMPORT/EXPORT</div>
  </div>
</div>

<script type="text/javascript">
// Настройки для приложения
window.FOC_CSV_PARAMS = {
  'requestConfig' : {
    'token': '{{ token }}',
    'tokenName': '{{ tokenName }}',
    'baseRoute': '{{ baseRoute }}',
    'baseUrl': '{{ baseUrl }}',
  },
  'language': '{{ language }}',
  'initial' : {
    data: {{ initial }}
  }
};
</script>
{% for script in scripts %}
  <script src="{{ script }}" type="text/javascript"></script>

{% endfor %}

{{ footer }}